<svg width="64" height="64" viewBox="0 0 64 64" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with Gradient -->
  <defs>
    <radialGradient id="grad-bg" cx="50%" cy="50%" r="50%" fx="50%" fy="50%">
      <stop offset="0%" style="stop-color:#1E293B;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0F172A;stop-opacity:1" />
    </radialGradient>
    <linearGradient id="grad-key" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FBBF24;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F59E0B;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="grad-lens" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#7DD3FC;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0EA5E9;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Main Shape -->
  <rect width="64" height="64" rx="12" fill="url(#grad-bg)"/>
  
  <!-- Key Symbol -->
  <g transform="translate(12, 12) scale(0.8) rotate(-15 32 32)">
    <!-- Key Head -->
    <circle cx="24" cy="24" r="10" fill="url(#grad-key)" stroke="#FDE68A" stroke-width="2"/>
    <circle cx="24" cy="24" r="5" fill="none" stroke="#FDE68A" stroke-width="2"/>
    <!-- Key Body -->
    <rect x="32" y="22" width="20" height="4" fill="url(#grad-key)" stroke="#FDE68A" stroke-width="1.5"/>
    <!-- Key Teeth -->
    <rect x="36" y="26" width="4" height="6" fill="url(#grad-key)" stroke="#FDE68A" stroke-width="1.5"/>
    <rect x="44" y="26" width="4" height="6" fill="url(#grad-key)" stroke="#FDE68A" stroke-width="1.5"/>
  </g>
  
  <!-- Magnifying Glass Symbol -->
  <g transform="translate(28, 28) scale(0.7) rotate(20 32 32)">
    <!-- Lens -->
    <circle cx="32" cy="32" r="12" fill="url(#grad-lens)" stroke="#E0F2FE" stroke-width="2.5" fill-opacity="0.8"/>
    <!-- Handle -->
    <rect x="18" y="42" width="8" height="18" rx="4" transform="rotate(-45 22 51)" fill="url(#grad-lens)" stroke="#E0F2FE" stroke-width="2"/>
  </g>
</svg>